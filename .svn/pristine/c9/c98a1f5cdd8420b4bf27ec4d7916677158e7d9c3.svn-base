html,body{
	margin: 0;
	padding: 0;
}
a,button,input,textarea{-webkit-tap-highlight-color: rgba(255, 255, 255, 0);}
.container{
	width: 100%;
	max-width: 768px;
	margin: 0 auto;
	display: table;
	font-family: Microsoft Yahei;
}

/************订单详情************/
.oi_1{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 15px;
	border-bottom: 1px solid #EEE;
	line-height: 20px;
	font-size: 16px;
}
.oi_1 span:nth-child(odd){
	width: 112px;
	float: left;
	color: #797E85;
	padding: 5px 0;
}
.oi_1 span:nth-child(even){
	width: calc(100% - 112px);
	float: left;
	color: #48505B;
	padding: 5px 0;
}
.oi_1_pinjia{
	width: 100%;
    height: 46px;
    float: left;
    line-height: 46px;
    text-align: center;
    text-decoration: none;
    background-color: #A9D1E0;
    margin: 10px 0 10px;
    border-radius: 4px;
    color: white;
}
.oi_2{
	width: 100%;
	height: 100%;
	float: left;
	box-sizing: border-box;
	padding: 10px 15px;
	border-bottom: 1px solid #EEE;
}
.oi_2_a{
	width: calc(100% - 90px);
	height: 100%;
	float: right;
	margin: 0;
}
.oi_2_a span:nth-child(1){
	width: 60%;
    height: 30px;
    float: left;
    color: #48505B;
}
.oi_2_a span:nth-child(2){
	width: 40%;
    height: 30px;
    float: left;
    text-align: right;
    color: #48505B;
}
.oi_2_a span:nth-child(3){
	width: 100%;
    float: left;
    line-height: 20px;
    font-size: 14px;
    color: #A1A4A9;
}
.oi_3{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 0 15px;
}
.oi_3_a{
	width: 100%;
    height: 46px;
    float: left;
    line-height: 46px;
    text-align: center;
    text-decoration: none;
    background-color: #E96388;
    margin: 10px 0 10px;
    border-radius: 4px;
    color: white;
}
.oi_3_b{
	width: 100%;
    height: 46px;
    float: left;
    line-height: 46px;
    text-align: center;
    background-color: #E7E7E7;
    margin: 10px 0 50px;
    border-radius: 4px;
    color: #A1A4A9;
}
.oi_3_c{
	width: 100%;
    height: 46px;
    float: left;
    line-height: 46px;
    text-align: center;
    text-decoration: none;
    border: 1px solid #799CA6;
    margin: 10px 0 50px;
    border-radius: 4px;
    color: #799CA6;
}

/************我的订单************/
.om_menu{
	width: 100%;
	height: 40px;
	float: left;
	background-color: white;
}
.om_menu a{
	width: 25%;
	height: 40px;
	float: left;
	text-align: center;
	color: #48505B;
}
.om_menu a span{
	height: 40px;
	display: table;
	margin: 0 auto;
	box-sizing: border-box;
	line-height: 38px;
	font-size: 14px;
	border-bottom: 2px solid #FFF;
}
.menustate{
	color: #E96388!important;
    border-bottom-color: #799CA6!important;
}
.om_show{
	width: 100%;
	float: left;
	margin-top: 15px;
}
.om_1{
	width: 100%;
	height: 40px;
	float: left;
	background-color: white;
	box-sizing: border-box;
	padding: 0 15px;
}
.om_1 span:nth-child(1){
	float: left;
	line-height: 40px;
	color: #48505B;
	font-size: 14px;
}
.om_1 span:nth-child(1) i{
	font: 14px/40px 'Microsoft Yahei';
	font-weight: bolder;
}
.om_1 span:nth-child(2){
	float: right;
	line-height: 40px;
	color: #E96388;
	font-size: 14px;
}
.om_2{
	width: 100%;
	float: left;
	background-color: white;
	border-top: 1px solid #EEE;
	border-bottom: 1px solid #EEE;
	box-sizing: border-box;
	padding: 0 15px;
}
.om_2_a{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 10px 0;
	border-bottom: 1px solid #EEE;
}
.om_2_a_p{
	width: calc(100% - 90px);
	height: 80px;
	float: right;
	margin: 0;
}
.om_2_a_p span:nth-child(1){
	width: 60%;
    height: 30px;
    float: left;
    color: #48505B;
}
.om_2_a_p span:nth-child(2){
	width: 40%;
    height: 30px;
    float: left;
    text-align: right;
    color: #48505B;
}
.om_2_a_p span:nth-child(3){
	width: 100%;
    float: left;
    line-height: 20px;
    font-size: 14px;
    color: #A1A4A9;
}
.om_2_b{
	width: 100%;
	height: 40px;
	float: left;
	color: #48505B;
	text-align: right;
	line-height: 40px;
	font-size: 14px;
}
.om_2_b i{
	font: 14px/40px 'Microsoft Yahei';
	color: #E96388;
}
.om_3{
	width: 100%;
	height: 60px;
	float: left;
	background-color: white;
	box-sizing: border-box;
	padding: 12px 15px;
}
.om_3 a:nth-child(1){
	width: 78px;
	height: 34px;
	float: right;
	font-size: 14px;
	line-height: 34px;
	text-align: center;
	border-radius: 4px;
	text-decoration: none;
}
.om_3 a:nth-child(2){
	width: 78px;
	height: 34px;
	float: right;
	font-size: 14px;
	line-height: 34px;
	text-align: center;
	border-radius: 4px;
	margin-right: 10px;
	text-decoration: none;
}
.om_3 a:nth-child(3){
	width: 78px;
	height: 34px;
	float: right;
	font-size: 14px;
	line-height: 34px;
	text-align: center;
	border-radius: 4px;
	margin-right: 10px;
	text-decoration: none;
}

.om_3_a{
	border: 1px solid #E96388;
	color: #E96388;
}
.om_3_b{
	border: 1px solid #A7A8AA;
	color: #A1A4A9;
}
.om_3_c{
	border: 1px solid #799CA6;
	color: #799CA6;
}
.om_3_d{
	border: 1px solid #A1A4A9;
	color: #A1A4A9;
}

.om_refund{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 10px 15px 30px;
	background-color: white;
	margin-top: 10px;
}
.om_refund p{
	width: 100%;
	height: 24px;
	float: left;
	margin: 0;
	color: #48505B;
	line-height: 24px;
}
.om_refund textarea{
	-webkit-appearance: none;
    width: 100%;
    height: 110px;
    line-height: 20px;
    float: left;
    resize: none;
    margin: 5px 0 20px;
    box-sizing: border-box;
    padding: 5px;
    outline: none;
    color: #A1A4A9;
    font: 15px/26px 'Microsoft Yahei';
    border-color: #CCC;
    border-radius: 4px;
}
.om_refund span{
	width: 100%;
    height: 46px;
    float: left;
    line-height: 46px;
    text-align: center;
    background-color: #799CA6;
    border-radius: 4px;
    color: white;
}

/************赠送好友************/
.orderreceive_1{
	width: 100%;
	height: 40px;
	border-bottom: 1px solid #EEE;
	color: #48505B;
	font-size: 14px;
	line-height: 40px;
	margin: 0;
	box-sizing: border-box;
	padding: 0 15px;
}
.orderreceive_1 span{
	font-weight: bolder;
}
.orderreceive_2{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 10px 15px;
	border-bottom: 1px solid #EEE;
}
.orderreceive_2 p{
	width: calc(100% - 90px);
    float: right;
    margin: 0;
    color: #48505B;
}
.orderreceive_3{
	width: 100%;
	height: 68px;
	float: left;
	box-sizing: border-box;
	padding: 16px 15px 24px;
}
.orderreceive_3 span{
	width: 80px;
	line-height: 34px;
	float: left;
}
.orderreceive_3 input{
	-webkit-appearance: none;
	width: calc(100% - 90px);
	height: 34px;
	float: right;
	outline: none;
	box-sizing: border-box;
	padding: 3px 10px;
	font: 15px/28px 'Microsoft Yahei';
	border: 1px solid #A1A4A9;
	color: #A1A4A9;
	border-radius: 4px;
}
.orderreceive_4{
	width: 100%;
	float: left;
	box-sizing: border-box;
	padding: 12px 15px;
}
.orderreceive_4 input[type='button']{
	-webkit-appearance: none;
	width: 100%;
	height: 46px;
	float: left;
	background-color: #448D88;
	padding: 5px 10px;
	outline: none;
	box-sizing: border-box;
	border-radius: 4px;
	color: #FFF;
	font: 18px/36px 'Microsoft Yahei';
	border: 0;
	font-weight: bolder;
}