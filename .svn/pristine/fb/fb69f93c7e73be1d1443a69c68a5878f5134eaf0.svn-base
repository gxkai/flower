<style>
.container {
    width: 100%;
    max-width: 768px;
    margin: 0 auto;
    display: table;
    font-family: Microsoft Yahei;
    padding: 0;
}
.content{
	background-color: #ffa0ca;
	padding: 5vw 2vw;
	text-align: center;
}
ul{
	margin: auto;
	width: 98%;
}
.content img{
	width: 100%;
}
li{
	width: 29%;
	display: inline-block;
	margin-bottom: 3vw;
}
li p{
	color: #fff;
	font-size: 12px;
}
.outer{
	position: relative;
	width: 26vw;
	height: 26vw;
	display: inline-block;
}
.outer div{
    position: absolute;
    transform-style: preserve-3d;
    -ms-transform-style: preserve-3d;	/* IE 9 */
	-moz-transform-style: preserve-3d;	/* Firefox */
	-webkit-transform-style: preserve-3d; /* Safari 和 Chrome */
	-o-transform-style: preserve-3d; 	/* Opera */
    backface-visibility:hidden;
    -webkit-backface-visibility:hidden;	/* Chrome 和 Safari */
	-moz-backface-visibility:hidden; 	/* Firefox */
	-ms-backface-visibility:hidden; 	/* Internet Explorer */
    transition:all 3s;
     -ms-transition: all 3s;	/* IE 9 */
	-moz-transition: all 3s;	/* Firefox */
	-webkit-transition: all 3s; /* Safari 和 Chrome */
	-o-transition: all 3s; 	/* Opera */
}
.div1{
    transform:rotateY(0deg);
	-ms-transform:rotateY(0deg); 	/* IE 9 */
	-moz-transform:rotateY(0deg);	/* Firefox */
	-webkit-transform:rotateY(0deg); /* Safari 和 Chrome */
	-o-transform:rotateY(0deg); 	/* Opera */
}
.div2{
    transform:rotateY(-180deg);
    -ms-transform:rotateY(-180deg); 	/* IE 9 */
	-moz-transform:rotateY(-180deg);	/* Firefox */
	-webkit-transform:rotateY(-180deg); /* Safari 和 Chrome */
	-o-transform:rotateY(-180deg); 	/* Opera */
}
.result{
	position: absolute;
	top: 50%;
    left: 27%;
    font-family: cursive;
   /*   animation: word 2s;
    -webkit-animation: word 2s;
    -o-animation: word 2s;
    -moz-animation: word 2s;  */
}
.result p{
 text-align: center;
 color: #f37317;
}
@keyframes word
{
	from {opacity: 0;}
	to {opacity: 1;}
}
@-wekit-keyframes word
{
	from {opacity: 0;}
	to {opacity: 1;}
}
@-moz-keyframes word
{
	from {opacity: 0;}
	to {opacity: 1;}
}
@-o-keyframes word
{
	from {opacity: 0;}
	to {opacity: 1;}
}
#showDiv{
	background-color: #3a3939;
	width:70vw;
	height:auto;
	display:none;
	z-index: 99;
	opacity: 0.9;
	margin-left: 8vw;
	text-align:center;
	margin-top: 60vw;
	color: #fff;
	font-size: 14px;
	padding: 5px;
}
.bottom{
	background-color: #ff6eaf;
    font-size: 13px;
    color: #fff;
}
.bottom_text{
	padding: 6vw 4vw 0vw 5vw;
}
.p1{
	text-align: center;
	color: #fff;
	font-family: cursive;
	margin: 3vw;
	position: relative;
	letter-spacing: 2px;
}
.p1:before, .p1:after {
    content: '';                 
    position: absolute;         
    top: 52%;
    background: #fff;       
    width: 9%;
    height: 1px;
}
.p1:before{
    left: 12%;        
}
.p1:after {
    right: 12%;
}
@media (min-width: 768px) {
	.outer {
	    width: 80%;
	    height: 170px;
	}
}
</style>
