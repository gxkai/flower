body{
	font-family:"Tahoma";
	font-size: 13px;
}
.table2{
	margin-top: 1.4cm;
	height: 7cm;
}
table {
	width: 9.5cm;
	height:3.2cm;
	margin: 0 auto;
	border: 1px solid #666;
	margin-top: 0.3cm;
	border-collapse:collapse;
}
table tr{
	border: 1px solid #666 ;
}
table tr td:nth-child(1){
	border: 1px solid #666 ;
	width: 1.7cm;
	text-align: center;
}
table tr td:nth-child(2){
	border: 1px solid #666 ;
	text-align: left;
	padding-left: 10px;
}
.zhan span{
	width: 33.3333333%;
	height: 60px;
	line-height: 60px;
	text-align: center;
	float: left;
}
.zhan span:nth-child(1){
	font-size: 17px;
}
.zhan span:nth-child(2){
	border-left: 1px solid #666 ;
	border-right: 1px solid #666 ;
	box-sizing: border-box;
	font-size: 40px;
	font-weight: bolder;
}
.zhan span:nth-child(3){
	font-size: 35px;
	font-family: YouYuan;
	font-weight: bolder;
}
[class^='barcode_']{
	width: 100%;
	height: 30px;
	float: left;
	margin-left: -9px;
}
[class^='number_']{
	width: 100%;
	height: 20px;
	float: left;
	box-sizing: border-box;
	padding-left: 10px;
	text-align: left;
	margin-left: -9px;
}
input[type='button']{
	width: 60px;
	height: 30px;
	position: fixed;
	bottom: 10px;
	right: 10px;
	cursor: pointer;
}
.qian{
	text-align: left;
	height: 1.5cm;
}
.qian span:nth-child(1){
	font-weight: bolder;
	margin-left: 3px;
	float: left;
}
.qian span:nth-child(2){
	float: right;
	margin-right: 30px;
}
