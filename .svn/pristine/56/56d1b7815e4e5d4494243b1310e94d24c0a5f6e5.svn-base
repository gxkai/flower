<style type="text/css">
    .bg{
		position:absolute;
		background-color:hsla(0,0%,50%,0.5); /*将背景设置为不透明度为0.5的黑色*/
		top:0;
		left:0;
	     }
	.box{ /*盒模型，可以认为是一个容器，其有内容、边框、填充等属性*/
		position:absolute;
		background-color:Red;
		top:130px;
		left:150px;
		z-index:99; /*元素的堆叠指数，数值越高，越位于上方，仅能在position:absolute在起效果*/
		border-radius:4px; /*用于定义模型的圆角边框*/
		padding:5px;
		background-color:#fff;
		line-height:20px;
		font-size:12px;
		color:#666;
		font-weight:bold;
	   }
	.box a{
		display:block;
		position:absolute;
		z-index:100;
		top:-11px;
		left:216px;
		border-radius:13px;
		border:2px solid #e4e4e4;
		background-color:#bbb;
		line-height:18px;
		width:18px;
		text-align:center;
		font-family:Arial, Helvetica, sans-serif;
		font-size:14px;
		color:#FFF;
		text-decoration:none;
	}
	.box img{
		width:220px;
		height:320px;
	}
	
</style>

<script type="text/javascript">
    var img='${img!}';
    $(function(){
    	$(document).snowfall('clear');
        $(document).snowfall({
            image: '/resource/flower/image/sign18/redpacket.png',
            flakeCount:30,
            minSize: 5,
            maxSize: 22
        });
    }) 
    window.onload =setTimeout( function (){
    	var child=$.cookie('${img!}');
    	/* alert(child); */
    	if(child==null){
    		
    		if(img!=''){
    			$.cookie('${img!}', 'y'); 
        		//body设置为fixed之后会飘到顶部，所以要动态计算当前用户所在高度
            	//相当于禁用滚动条
            	var scrollTop = $("body").scrollTop();
            	$("body").css({
            	    'overflow':'hidden',
            	    'position': 'fixed',
            	    'top': scrollTop*-1
            	});

                var w = window.innerWidth; //窗口文档显示区的宽度
                var h = window.innerHeight;

                var bgdiv = document.createElement("div");
                bgdiv.style.width = document.body.scrollWidth + "px";
                bgdiv.style.height =document.body.scrollHeight + "px";
                bgdiv.className = "bg";
                bgdiv.id = "bg";
                document.body.appendChild(bgdiv);

                //创建图片预览层
                var box = document.createElement("div");
                box.id = "floatbox";
                box.className = "box";
                box.innerHTML = "<a href='javascript:closebox()'>&times</a>";
                box.innerHTML += "<img src=" + img+ " />";
                //图片预览居中
                box.style.left = (w - 220) / 2 + "px";
                box.style.top = (h - 320) / 2 + "px";
                document.body.appendChild(box);
            } 
    	}
    	/* $.cookie('isChild', 'n',{path: "/account/center"}); */ 
    	/* alert($.cookie('isChild')); */
    	
    	
    	wxconfig(window.location);
    	wx.ready(function(){
			
    		/* 分享到朋友圈  */
			wx.onMenuShareTimeline({
				title: '花美美', // 分享标题
			    desc: '用鲜花传递爱', // 分享描述
			    link: 'http://' +window.location.host+window.location.pathname, // 分享链接
			    imgUrl: 'http://' + window.location.host+'${img2!}' , // 分享图标
			    type: 'link', // 分享类型,music、video或link，不填默认为link
			    dataUrl: '', // 如果type是music或video，则要提供数据链接，默认为空
			    success: function () {
			        //用户确认分享后执行的回调函数
			        //判断是否是第18位分享的，如果是，则赠送18粒花籽，弹出提示框，否则什么也不做
			        
			        $.post('/account/sendSeeds18',function(data){
			        	if(data){
			        		alert('duang ! 今天太幸运了，成为了第18位签到大侠，被18粒花籽砸中！');
			        	}
			        });
			    },
			    cancel: function () {
			        // 用户取消分享后执行的回调函数
			    }
			});
    		
			/* 分享给好友  */
			wx.onMenuShareAppMessage({
			    title: '花美美', // 分享标题
			    desc: '用鲜花传递爱', // 分享描述
			    link: 'http://' +window.location.host+window.location.pathname, // 分享链接
			    imgUrl: 'http://' + window.location.host+'${img2!}' , // 分享图标
			    type: 'link', // 分享类型,music、video或link，不填默认为link
			    dataUrl: '', // 如果type是music或video，则要提供数据链接，默认为空
			    success: function () {
			        //用户确认分享后执行的回调函数
			        //判断是否是第18位分享的，如果是，则赠送18粒花籽，弹出提示框，否则什么也不做
			        
			        $.post('/account/sendSeeds18',function(data){
			        	if(data){
			        		alert('duang ! 今天太幸运了，成为了第18位签到大侠，被18粒花籽砸中！');
			        	}
			        });
			    },
			    cancel: function () {
			        // 用户取消分享后执行的回调函数
			    }
			});
		});
    	
    },100);
    //关闭图片预览
    function closebox() {
    	/*取消后设置回来*/
    	$("body").css({
    	    'overflow':'auto',
    	    'position': 'static',
    	    'top': 'auto'
    	});
       
        var bg = document.getElementById("bg");
        bg.parentNode.removeChild(bg);
        var box = document.getElementById("floatbox");
        box.parentNode.removeChild(box);
    }
  
    //点击头像显示 等级规则
    function showrule() {
    	var scrollTop = $("body").scrollTop();
    	$("body").css({
    	    'overflow':'hidden',
    	    'position': 'fixed',
    	    'top': scrollTop*-1
    	});

        var w = window.innerWidth; //窗口文档显示区的宽度
        var h = window.innerHeight;

        var bgdiv = document.createElement("div");
        bgdiv.style.width = document.body.scrollWidth + "px";
        bgdiv.style.height =document.body.scrollHeight + "px";
        bgdiv.className = "bg";
        bgdiv.id = "bg";
        document.body.appendChild(bgdiv);

        //创建图片预览层
        var box = document.createElement("div");
        box.id = "floatbox";
        box.className = "box";
        box.innerHTML = "<a href='javascript:closebox()'>&times</a>";
        box.innerHTML += "<img src='/resource/flower/image/click_head.jpg' />";
        //图片预览居中
        box.style.left = (w - 220) / 2 + "px";
        box.style.top = (h - 320) / 2 + "px";
        document.body.appendChild(box); 
        
    }
    function refresh() {
		$.post("/account/resetUserInfo",function(data){
			if (data) {
				location.replace("/account/center");
			}
		})
	}
    
