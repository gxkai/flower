			<form id="form" action="/account/saveAims" method="post" hidden>
				<input id="contentStr" name="contentStr" value="小赚一个亿"/>
			</form>
			<script>
			var flat = 1;
			$('.choose_img').click(function(){
				var id = $(this).data("img")
				console.log(id)
				if(id =="1"){contentStr ="小赚100万"}
				// 修改
				if(id =="2"){contentStr ="事业顺利</br>薪水翻倍"}
				if(id =="3"){contentStr ="不再熬夜"}
				// 修改
				if(id =="4"){contentStr ="来一场说走就走的旅行"}
				// 修改
				if(id =="5"){contentStr ="找个可以结婚的男朋友"}
				if(id =="6"){contentStr ="择一城与一人终老"}
				if(id =="7"){contentStr ="练出马甲线</br>体重减10斤"}
				if(id =="8"){contentStr ="拥有自己的房和车"}
				if(id =="9"){contentStr ="看100本书</br>或听100堂课程"}
				// 修改
				if(id =="10"){contentStr ="生个萌娃阖家欢乐"}
				
				$('#contentStr').val(contentStr)
				
				console.log($('#contentStr').val())
				
				// 加锁
				if(flat == 1){
					flat = 0;
					
					// 判断活动是否关闭
					$.ajax({
						url: '/account/isClose',
						type: 'post',
						dataType: 'json',
						cache: false,
						success: function(data){
							flat = 1;
							if(data==true){
								//提交目标
								// 判断是否可以制定目标
								$.ajax({
									url: '/account/isAllowSaveAims',
									type: 'post',
									dataType: 'json',
									cache: false,
									success: function(data){
										flat = 1;
										if(data==true){
											//提交目标
											$('#form').submit();
										}else{
											layer.msg('你已经制定过小目标了，去平台查看自己的分享海报');
										}
									}
								});
							}else{
								layer.msg('客官,来的不巧，小目标活动暂停了');
							}
						}
					});
					
					
				}else{
					layer.msg('服务器繁忙！');
				}
				
			})
			</script>
