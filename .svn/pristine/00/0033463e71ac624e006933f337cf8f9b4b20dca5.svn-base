			$(function(){
				$('#dowebok').fullpage({
					
					anchors: ['page1', 'page2', 'page3', 'page4','page5', 'page6', 'page7', 'page8','page9', 
					'page10','page11'],
					menu: '#menu',
					//滚动到某一屏后的回调函数
					afterLoad: function(anchorLink, index){
						var numName = ".img_num_" + index 
						var contentImgLeft = ".content_left_" + index
						var contentImgRight = ".content_right_" + index
						var aimText = "#aim_text_" + index
						var btnImg = ".btn_img_" + index
						
						//图片序列
						$(numName).delay(500).animate({
							left: '40%'
						}, 1000, 'easeOutExpo');
						//左边图片
						$(contentImgLeft).delay(500).animate({
							left: '0'
						}, 1000, 'easeOutExpo');
						//右边图片
						$(contentImgRight).delay(500).animate({
							right: '10%'
						}, 1000, 'easeOutExpo');	
						//下方按钮
						$(btnImg).delay(500).animate({
							bottom: '0'
						}, 1000, 'easeOutExpo');
						
//						$(aimText).removeClass('animated infinite bounce')
						
					},
					//滚动前的回调函数
					onLeave: function(index, direction){

						var numName = ".img_num_" + index 
						var contentImgLeft = ".content_left_" + index
						var contentImgRight = ".content_right_" + index
						var aimText = ".aim_text_" + index
						var btnImg = ".btn_img_" + index
						
						//图片序列
						$(numName).delay(500).animate({
							bottom: '-120%'
						}, 1000, 'easeOutExpo');
						//左边图片
						$(contentImgLeft).delay(500).animate({
							left: '-120%'
						}, 1000, 'easeOutExpo');
						//右边图片
						$(contentImgRight).delay(500).animate({
							right: '-120%'
						}, 1000, 'easeOutExpo');	
						//下方按钮
						$(btnImg).delay(500).animate({
							bottom: '-120%'
						}, 1000, 'easeOutExpo');
						
//						$(aimText).addClass('animated infinite bounce')
						
					}
				});
			});
		</script>

