	<body> 
		<div class="container">
			<div class="dh_product">
				<p><span>双品花束</span><span>所需花籽:40</span></p>
				<a onclick="dhhs(1)">兑换</a>
			</div>
			<div class="dh_product">
				<p><span>多品花束</span><span>所需花籽:60</span></p>
				<a onclick="dhhs(2)">兑换</a>
			</div>			
		</div>
		<script type="text/javascript">
			 function dhhs(id){
				$.ajax({
					url: '/service/exchangevalid/'+id,
					data: $('form').serialize(),
					type: 'post',
					dataType: 'json',
					cache: false,
					success: function(data){
						if(data){
							location.replace('/service/exchangeflower?type=4&pid=' + id);
						}else{
							layer.msg('您的花籽不足', {time: 2000});return;
						}
					}
				});				
			} 
			
