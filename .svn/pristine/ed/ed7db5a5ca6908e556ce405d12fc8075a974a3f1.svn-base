@CHARSET "UTF-8";

   /*  通用的css样式 
	*  @author glaicer
	*  @date 2017-09-19
	*/
	
/* 超过长度的隐藏 */
.span_describe{
	display:inline-block;
	width:100%;
	font-size: 12px;
	color: #cfcfcf;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}

/* span 做按钮 */
.span_btn{
	width: 20%;
    font-size: 12px;
    text-align: center;
    padding: 3px 4px;
    border-radius: 2px;
    border: 1px solid #ccc;
	color: #6b6b6b;
    text-decoration: none;
	outline:none;
}


